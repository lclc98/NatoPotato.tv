{
  "name": "natopotato",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build --prod --build-optimizer --vendor-chunk=true ",
    "build:stats": "ng build --prod --build-optimizer --vendor-chunk=true --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "yarn build:stats && webpack-bundle-analyzer dist/natopotato/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@fortawesome/angular-fontawesome": "~0.5.0",
    "@fortawesome/fontawesome-svg-core": "~1.2.22",
    "@fortawesome/free-brands-svg-icons": "~5.10.2",
    "@fortawesome/free-solid-svg-icons": "~5.10.2",
    "@js-joda/core": "~1.11.0",
    "@js-joda/timezone": "~2.1.1",
    "bulma": "git://github.com/jgthms/bulma#ff17a85faa",
    "ngx-cookie-service": "~2.2.0",
    "ngx-countdown": "~3.2.0",
    "rxjs": "~6.5.2",
    "tslib": "~1.10.0",
    "twitch": "~3.4.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "protractor": "~5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "~3.4.1"
  }
}
